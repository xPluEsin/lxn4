-- Scripts 

GreatWhite = false
LocalPlayer = game.Players.LocalPlayer.Name


Weapons:TweenPosition(UDim2.new(0.362894863, 0, 0.13691932, 0), "In")
Boats:TweenPosition(UDim2.new(0.476935029, 0, 0.13691932, 0), "In")
Sharks:TweenPosition(UDim2.new(0.590979397, 0, 0.136308074, 0), "In")
wmods:TweenPosition(UDim2.new(0.705733478, 0, 0.136308074, 0), "In")

function onKeyPress(inputObject, gameProcessedEvent)
    if inputObject.KeyCode == Enum.KeyCode.RightControl then
        if SharkBitmeh.Enabled == true then
            Boats:TweenPosition(UDim2.new(0.447815508, 0, -0.212713927, 0), "Out")
            Sharks:TweenPosition(UDim2.new(0.447815508, 0, -0.212713927, 0), "Out")
            Weapons:TweenPosition(UDim2.new(0.447815508, 0, -0.212713927, 0), "Out")
            wmods:TweenPosition(UDim2.new(0.447815508, 0, -0.212713927, 0), "Out")
            repeat
                wait()
                if Boats.Position == UDim2.new(0.447815508, 0, -0.212713927, 0) then
            SharkBitmeh.Enabled = false
                end
                until Boats.Position == UDim2.new(0.447815508, 0, -0.212713927, 0)
        else
            SharkBitmeh.Enabled = true
            Boats:TweenPosition(UDim2.new(0.476935029, 0, 0.13691932, 0), "In")
            Sharks:TweenPosition(UDim2.new(0.590979397, 0, 0.136308074, 0), "In")
            Weapons:TweenPosition(UDim2.new(0.362894863, 0, 0.13691932, 0), "In")
            wmods:TweenPosition(UDim2.new(0.705733478, 0, 0.136308074, 0), "In")
        end
    end
end
game:GetService("UserInputService").InputBegan:connect(onKeyPress)


function guns(gun)
    local remote = Game["Workspace"]["Events"]["Shop"]["ChangeActiveGun"]
local args = {
    [1] = gun
}
remote:FireServer(unpack(args))
end

function boats(boat)
    local remote = Game["Workspace"]["Events"]["MatchMaking"]["PartyChange"]
local args = {
    [1] = {
        Guests = {},
        Owner = LocalPlayer,
        IsInOtherParty = "",
        BoatId = boat
    }
}
remote:FireServer(unpack(args))
end

function gmods(mod)
    -- Synapse Only
-- Made by: Mod3rn#1999
-- Using Racist Dolphin#5199 Dev Console <3

local ps = game:GetService("Players")
local i = game:GetService("UserInputService")
local r = game:GetService("RunService")
local cg = game:GetService("CoreGui")
local p = ps.LocalPlayer
local c = p.Character
local b = p:FindFirstChild("Backpack") or p:WaitForChild("Backpack")
local mo = p:GetMouse()

p.CharacterAdded:connect(function(char)
   c = char
end)

local m = getrawmetatable(game)
make_writeable(m)

local oldindex = m.__index
local oldnamecall = m.__namecall

m.__namecall = function(...)
local t = {...}
local method = t[#t]

if method == "Play" then
local env = getfenv(2)
if env.script then
if env.script.Parent ~= nil then

debug.setupvalue(2, mod, math.huge)
end
end
end

return oldnamecall(...)
end
end

function speedb(num)
    local ps = game:GetService("Players")
local i = game:GetService("UserInputService")
local r = game:GetService("RunService")
local cg = game:GetService("CoreGui")
local p = ps.LocalPlayer
local c = p.Character
local b = p:FindFirstChild("Backpack") or p:WaitForChild("Backpack")
local mo = p:GetMouse()

p.CharacterAdded:connect(function(char)
    c = char
end)

local m = getrawmetatable(game)
make_writeable(m)

local oldindex = m.__index
local oldnamecall = m.__namecall

m.__namecall = function(...)
    local t = {...}
    local method = t[#t]

    if method == "FindFirstChild" then
        if t[2] == "SharkBoost" then
            local env = getfenv(2)
                if env.script then
                    if env.script.Parent ~= nil then
                    debug.setupvalue(2, "speed", num)
                end
            end
        end
    end

    return oldnamecall(...)
end
end

bdash.MouseButton1Down:connect(function()
    if bs.Size == UDim2.new(0, 197, 0, 135) then
        bdash.Rotation = 90
        bs:TweenSize(UDim2.new(0, 197, 0, 0), "In")
        repeat
            wait()
            if bs.Size == UDim2.new(0, 197, 0, 0) then
                bs.Visible = false
            end
        until bs.Size == UDim2.new(0, 197, 0, 0)
    else
        bs.Visible = true
        wait()
    bs:TweenSize(UDim2.new(0, 197, 0, 135), "Out")
    bdash.Rotation = 0
    end
end)

