kss.MouseButton1Click:connect(function()
    -- Creds to RelentlessRaptor#5709
    for _, v in pairs(game.Players:GetChildren()) do
        if v.TeamColor == game.Teams.Shark.TeamColor then
            for i = 1, 10000 do
                 game.Workspace.Events.Projectiles.HealthChange:FireServer(v.Name, 20000, i)
            end
        end
    end
kss.TextColor3 = Color3.new(1, 0.2, 0.345098)
wait(2)
kss.TextColor3 = Color3.new(255, 255, 255)
end)

gss.MouseButton1Click:connect(function()
    -- Creds to RelentlessRaptor#5709
    for i, v in pairs(game.Players:GetChildren()) do
        if v.TeamColor == game.Teams.Shark.TeamColor then
            for i = 1, 10000 do
                game.Workspace.Events.Projectiles.HealthChange:FireServer(game.Players.LocalPlayer.Name, -200000, i)
            end
        end
    end
gss.TextColor3 = Color3.new(1, 0.2, 0.345098)
repeat
    wait()
    if game.Players.LocalPlayer.Team ~= game.Teams.Shark then
        gss.TextColor3 = Color3.new(255, 255, 255)
    end
until game.Players.LocalPlayer.Team ~= game.Teams.Shark
end)
