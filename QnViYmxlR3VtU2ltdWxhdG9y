			if v.Name ~= "Log" then
				v.Visible = false
			end
		end
	else
		LogText("Click to Close")
		for i,v in pairs(BubbleGUI:GetChildren()) do
			if v.Name ~= "Log" then
				v.Visible = true
			end
		end
	end
end)

