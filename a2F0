KnifeAbilityTestButton3.MouseButton1Click:connect(function()
loadstring(game:HttpGet(('https://pastebinp.com/raw/HbcK08EE'),true))()
end)

KnifeAbilityTestButton4.Name = "KnifeAbilityTestButton4"
KnifeAbilityTestButton4.Parent = KnifeAbilityTestFrame
KnifeAbilityTestButton4.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestButton4.BackgroundTransparency = 0.55000001192093
KnifeAbilityTestButton4.Position = UDim2.new(0.157534242, 0, 0.725792289, 0)
KnifeAbilityTestButton4.Size = UDim2.new(0, 200, 0, 22)
KnifeAbilityTestButton4.Font = Enum.Font.Arcade
KnifeAbilityTestButton4.Text = "Music"
KnifeAbilityTestButton4.TextColor3 = Color3.new(0, 0, 0)
KnifeAbilityTestButton4.TextSize = 14

KnifeAbilityTestButton4.MouseButton1Click:connect(function()
	if KnifeAbilityTestButton4.Text == "Music: off" then
      KnifeAbilityTestButton4.Text = "Music: on"
      KnifeAbilityTestButton4.TextColor3 = Color3.new(0,185,0)
   s = Instance.new("Sound")
   s.Name = "Music"
   s.SoundId = 'rbxassetid://4569332872'
   s.Looped = true
   s.Pitch = 1
   s.Volume = 1
   s.Archivable = false
   s.Parent = game.Workspace
   wait(1)
   s:Play()
else 
     KnifeAbilityTestButton4.Text = "Music: off"
     KnifeAbilityTestButton4.TextColor3 = Color3.new(0, 185, 0)
   wait(1)
   s:Stop()
end
end)
