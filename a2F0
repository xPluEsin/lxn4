---- Double Click On Music To Play Music ----

local Workspace = game:GetService("Workspace")
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local KnifeAbilityTest = Instance.new("ScreenGui")
local KnifeAbilityTestFrame = Instance.new("Frame")
local KnifeAbilityTestLabel = Instance.new("TextLabel")
local KnifeAbilityTestButton1 = Instance.new("TextButton")
local KnifeAbilityTestButton2 = Instance.new("TextButton")
local KnifeAbilityTestButton3 = Instance.new("TextButton")
local KnifeAbilityTestButton4 = Instance.new("TextButton")

KnifeAbilityTest.Name = "KnifeAbilityTest"
KnifeAbilityTest.Parent = game.CoreGui


KnifeAbilityTestFrame.Name = "KnifeAbilityTestFrame"
KnifeAbilityTestFrame.Parent = KnifeAbilityTest
KnifeAbilityTestFrame.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestFrame.BackgroundTransparency = 0.75
KnifeAbilityTestFrame.Position = UDim2.new(0.0273972396, 0, 0.161290318, 0)
KnifeAbilityTestFrame.Size = UDim2.new(0, 292, 0, 150)
KnifeAbilityTestFrame.Draggable = true

KnifeAbilityTestLabel.Name = "KnifeAbilityTestLabel"
KnifeAbilityTestLabel.Parent = KnifeAbilityTestFrame
KnifeAbilityTestLabel.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestLabel.BackgroundTransparency = 0.64999997615814
KnifeAbilityTestLabel.Position = UDim2.new(0.089041099, 0, 0.0364145674, 0)
KnifeAbilityTestLabel.Size = UDim2.new(0, 240, 0, 32)
KnifeAbilityTestLabel.Font = Enum.Font.Arcade
KnifeAbilityTestLabel.Text = "Knife Ability Test GUI"
KnifeAbilityTestLabel.TextColor3 = Color3.new(0, 0, 0)
KnifeAbilityTestLabel.TextSize = 14


KnifeAbilityTestButton1.Name = "KnifeAbilityTestButton1"
KnifeAbilityTestButton1.Parent = KnifeAbilityTestFrame
KnifeAbilityTestButton1.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestButton1.BackgroundTransparency = 0.55000001192093
KnifeAbilityTestButton1.Position = UDim2.new(0.157534242, 0, 0.289461941, 0)
KnifeAbilityTestButton1.Size = UDim2.new(0, 200, 0, 22)
KnifeAbilityTestButton1.Font = Enum.Font.Arcade
KnifeAbilityTestButton1.Text = "Click Here First!"
KnifeAbilityTestButton1.TextColor3 = Color3.new(0, 0, 0)
KnifeAbilityTestButton1.TextSize = 14

KnifeAbilityTestButton1.MouseButton1Click:connect(function()
loadstring(game:HttpGet(('https://pastebinp.com/raw/B8D1yJZt'),true))()
end)

KnifeAbilityTestButton2.Name = "KnifeAbilityTestButton2"
KnifeAbilityTestButton2.Parent = KnifeAbilityTestFrame
KnifeAbilityTestButton2.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestButton2.BackgroundTransparency = 0.55000001192093
KnifeAbilityTestButton2.Position = UDim2.new(0.157534242, 0, 0.436869293, 0)
KnifeAbilityTestButton2.Size = UDim2.new(0, 200, 0, 22)
KnifeAbilityTestButton2.Font = Enum.Font.Arcade
KnifeAbilityTestButton2.Text = "Then Click Me !"
KnifeAbilityTestButton2.TextColor3 = Color3.new(0, 0, 0)
KnifeAbilityTestButton2.TextSize = 14

KnifeAbilityTestButton2.MouseButton1Click:connect(function()
loadstring(game:HttpGet(('https://pastebinp.com/raw/rZqei1xh'),true))()
end)

KnifeAbilityTestButton3.Name = "KnifeAbilityTestButton3"
KnifeAbilityTestButton3.Parent = KnifeAbilityTestFrame
KnifeAbilityTestButton3.BackgroundColor3 = Color3.new(1, 1, 1)
KnifeAbilityTestButton3.BackgroundTransparency = 0.55000001192093
KnifeAbilityTestButton3.Position = UDim2.new(0.157534242, 0, 0.583989263, 0)
KnifeAbilityTestButton3.Size = UDim2.new(0, 200, 0, 22)
KnifeAbilityTestButton3.Font = Enum.Font.Arcade
KnifeAbilityTestButton3.Text = "ESP"
KnifeAbilityTestButton3.TextColor3 = Color3.new(0, 0, 0)
